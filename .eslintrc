{
    "extends": [
        "eslint:recommended",
        "problems"
    ],
    "env": {
        "browser": true
    },
    "globals": {
        "VERSION": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-cond-assign": "off",
        "no-console": "error",
        "keyword-spacing": "warn",
        "eqeqeq": "error"
    }
}
